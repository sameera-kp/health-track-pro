generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id          Int      @id @default(autoincrement())
  name        String
  age         Int?
  blood_group String?
  condition   String?
  last_visit  DateTime @default(now())

  @@map("patients") 
}